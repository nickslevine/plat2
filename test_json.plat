use std::json;

fn main() -> Int32 {
  let result: Result<json::JsonValue, String> = json::parse(input = "null");

  let value: json::JsonValue = match result {
    Result::Ok(v: json::JsonValue) -> v,
    Result::Err(err: String) -> {
      print(value = "Parse error: ${err}");
      return 1;
    }
  };

  let json_str: String = json::stringify(value = value);
  print(value = "JSON: ${json_str}");

  return 0;
}
