class Point {
  let x: i32;
  let y: i32;
  var name: string;

  init(x: i32, y: i32, name: string) {
    self.x = x;
    self.y = y;
    self.name = name;
  }

  fn add(other: Point) -> Point {
    return Point(x = self.x + other.x, y = self.y + other.y, name = "sum");
  }
}

fn main() {
  let p1 = Point(x = 10, y = 20, name = "first");
  let p2 = Point(x = 5, y = 15, name = "second");
  let sum = p1.add(p2);
  print("Done");
}