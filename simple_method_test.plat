class Point {
  let x: Int32;
  var name: String;

  init(x: Int32, name: String) {
    self.x = x;
    self.name = name;
  }

  fn change_name(new_name: String) {
    self.name = new_name;
  }
}

fn main() {
  let p1 = Point.init(x = 10, name = "first");
  p1.change_name(new_name = "changed");
  print(value = "Done");
}