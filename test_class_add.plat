class Point {
  let x: i32;
  let y: i32;

  init(x: i32, y: i32) -> Point {
    self.x = x;
    self.y = y;
    return self;
  }

  fn add(other: Point) -> Point {
    return Point(x = self.x + other.x, y = self.y + other.y);
  }
}

fn main() {
  let p1 = Point(x = 10, y = 20);
  let p2 = Point(x = 5, y = 15);
  let sum = p1.add(p2);
  print("Done");
}