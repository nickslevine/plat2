// Simple test for vtable infrastructure

class Animal {
  let name: string;

  init(name: string) -> Animal {
    self.name = name;
    return self;
  }

  fn get_name() -> string {
    return self.name;
  }
}

fn main() -> i32 {
  let animal = Animal(name = "Test");
  let name = animal.get_name();
  print("Name: ${name}");
  return 0;
}
