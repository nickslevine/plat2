fn main() -> Int32 {
  print(value = "Starting concurrent test with scope tracking...");

  concurrent {
    let task1: Task<Int32> = spawn {
      return 42;
    };

    let task2: Task<Int32> = spawn {
      return 100;
    };

    let result1: Int32 = task1.await();
    let result2: Int32 = task2.await();

    print(value = "Task 1 result: ${result1}");
    print(value = "Task 2 result: ${result2}");
  }

  print(value = "All tasks completed!");
  return 0;
}
