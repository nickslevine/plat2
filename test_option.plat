fn main() {
    // Test Option::Some
    let some_value = Option::Some(42);
    let result = match some_value {
        Option::Some(x) -> x + 10,
        Option::None -> 0
    };
    print("Some(42) + 10 = ${result}");

    // Test Option::None
    let none_value = Option::None;
    let none_result = match none_value {
        Option::Some(x) -> x,
        Option::None -> -1
    };
    print("None = ${none_result}");

    // Test with string
    let some_string = Option::Some("Hello");
    let string_result = match some_string {
        Option::Some(s) -> s,
        Option::None -> "Nothing"
    };
    print("Some string: ${string_result}");
}