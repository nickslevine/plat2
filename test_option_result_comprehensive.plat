fn test_option_int() -> i32 {
    let some_val = Option::Some(42);
    let result = match some_val {
        Option::Some(x) -> x,
        Option::None -> 0
    };
    return result;
}

fn test_option_string() -> string {
    let some_str = Option::Some("Hello");
    let result = match some_str {
        Option::Some(s) -> s,
        Option::None -> "Nothing"
    };
    return result;
}

fn test_result_int() -> i32 {
    let ok_val = Result::Ok(100);
    let result = match ok_val {
        Result::Ok(x) -> x,
        Result::Err(e) -> e
    };
    return result;
}

fn main() {
    let int_result = test_option_int();
    print("Option<i32>: ${int_result}");

    let string_result = test_option_string();
    print("Option<String>: ${string_result}");

    let result_result = test_result_int();
    print("Result<i32,i32>: ${result_result}");
}