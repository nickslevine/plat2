class Point {
  let x: i32;
  var name: string;

  init(x: i32, name: string) -> Point {
    self.x = x;
    self.name = name;
    return self;
  }

  fn change_name(new_name: string) {
    self.name = new_name;
  }
}

fn main() {
  let p1 = Point(x = 10, name = "first");
  p1.change_name("second");
  print("Done");
}