// Compare type alias (interchangeable) vs newtype (distinct)
type AliasID = i32;
newtype NewtypeID = i32;

fn take_alias(x: AliasID) -> i32 {
  return x + 10;
}

fn take_newtype(x: NewtypeID) -> i32 {
  return 20;
}

fn main() -> i32 {
  // Type alias: can pass raw i32 (interchangeable)
  let result1 = take_alias(42);

  // Newtype: cannot pass raw i32 (distinct type)
  // This line should fail:
  let result2 = take_newtype(42);

  return result1 + result2;
}
