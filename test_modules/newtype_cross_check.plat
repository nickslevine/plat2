// Test that different newtypes with same underlying type are distinct
newtype DocumentID = I32;
newtype UserID = I32;
type PlainID = I32;

fn process_doc(id: DocumentID) -> I32 {
  return 100;
}

fn process_user(id: UserID) -> I32 {
  return 200;
}

fn process_plain(id: PlainID) -> I32 {
  return 300;
}

fn main() -> I32 {
  // These should all work: type alias is interchangeable
  let plain1 = process_plain(1);
  let plain2 = process_plain(2);
  let plain3 = process_plain(3);

  return plain1 + plain2 + plain3;
}
