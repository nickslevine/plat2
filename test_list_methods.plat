fn main() -> i32 {
    // Test new List methods
    let numbers: List[i32] = [10, 20, 30, 40, 50];

    // Test length method
    let len = numbers.length();
    print("Length: ${len}");

    // Test contains method
    let has_30 = numbers.contains(30);
    print("Contains 30: ${has_30}");

    // Test index_of method
    let maybe_index = numbers.index_of(30);
    print("Index of 30: ${maybe_index}");

    // Test count method
    let count_30 = numbers.count(30);
    print("Count of 30: ${count_30}");

    // Test slice method
    let slice = numbers.slice(1, 4);
    print("Slice [1:4]: ${slice}");

    // Test all method
    let all_positive = numbers.all(0); // Simplified: all are > 0 (truthy)
    print("All positive: ${all_positive}");

    // Test any method
    let any_big = numbers.any(0); // Simplified: any are > 0 (truthy)
    print("Any big: ${any_big}");

    return 0;
}