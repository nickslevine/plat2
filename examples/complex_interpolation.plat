fn square(x: Int32) -> Int32 {
  return x * x;
}

fn main() -> Int32 {
  let base: Int32 = 5;
  let multiplier: Int32 = 3;
  let offset: Int32 = 10;

  // Complex expression interpolation
  print(value = "Base: ${base}, Squared: ${square(x = base)}");
  print(value = "Complex: ${(base + multiplier) * 2 - offset}");
  print(value = "Nested: Result is ${base * multiplier + square(x = 2)}");

  return 0;
}