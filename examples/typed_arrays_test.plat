fn main() {
  // Test different homogeneous array types

  // i32 array
  let numbers = [1, 2, 3, 4, 5];
  print("i32 array: ${numbers}");
  print("First number: ${numbers[0]}");
  print("Array length: ${numbers.len()}");

  // bool array
  let flags = [true, false, true, false];
  print("bool array: ${flags}");
  print("First flag: ${flags[0]}");
  print("Flags length: ${flags.len()}");

  // string array
  let words = ["hello", "world", "from", "plat"];
  print("string array: ${words}");
  print("First word: ${words[0]}");
  print("Words length: ${words.len()}");

  // Test iterations
  print("Iterating numbers:");
  for (num in numbers) {
    print("Number: ${num}");
  }

  print("Iterating flags:");
  for (flag in flags) {
    print("Flag: ${flag}");
  }

  print("Iterating words:");
  for (word in words) {
    print("Word: ${word}");
  }
}