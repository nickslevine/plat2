fn main() -> Int32 {
  // Test string literals
  print(value = "Testing Plat language features...");

  // Test arithmetic
  let x: Int32 = 5 + 3;
  let y: Int32 = 10 - 2;

  // Test comparisons
  if (x == y) {
    print(value = "Arithmetic works: 5 + 3 == 10 - 2");
  }

  // Test boolean operations with short circuit
  let a: Bool = true;
  let b: Bool = false;

  if (a or b) {
    print(value = "Boolean OR works");
  }

  if (not b) {
    print(value = "Boolean NOT works");
  }

  print(value = "All tests passed!");
  return 0;
}