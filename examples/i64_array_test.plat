fn main() {
  // Test large numbers that require i64
  let big_numbers = [2147483648, 4294967296, 1000000000000];
  print("Big numbers array: ${big_numbers}");
  print("First big number: ${big_numbers[0]}");
  print("Array length: ${big_numbers.len()}");

  // Iterate through them
  for (num in big_numbers) {
    print("Big number: ${num}");
  }
}