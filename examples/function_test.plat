fn add(x: Int32, y: Int32) -> Int32 {
  return x + y;
}

fn multiply(x: Int32, y: Int32) -> Int32 {
  return x * y;
}

fn main() {
  let a: Int32 = 6;
  let b: Int32 = 6;
  let sum: Int32 = add(x = a, y = b);
  let product: Int32 = multiply(x = sum, y = 2);
  print(value = "The result of product is ${product}");
}
