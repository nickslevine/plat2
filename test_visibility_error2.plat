// Test that private field access is caught
class BankAccount {
  let balance: Int32;  // Private field
  pub let owner: String;  // Public field
}

fn main() -> Int32 {
  let account: BankAccount = BankAccount.init(balance = 1000, owner = "Alice");

  // This should fail - accessing private field directly
  let bal: Int32 = account.balance;
  print(value = "Balance: ${bal}");

  return 0;
}
