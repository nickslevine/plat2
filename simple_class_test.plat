class Point {
  let x: Int32;
  let y: Int32;

  init(x: Int32, y: Int32) -> Point {
    self.x = x;
    self.y = y;
    return self;
  }

  fn get_magnitude() -> Int32 {
    return self.x * self.x + self.y * self.y;
  }
}

fn main() {
  let p1: Point = Point.init(x = 10, y = 20);
  let magnitude: Int32 = p1.get_magnitude();
  print(value = "Magnitude: ${magnitude}");
}